0","141"],"import \"../styles/Landing.css\";\nimport imgIncu from \"../assets/téléchargement.jpeg\"\n// src/pages/LandingPage.js\nimport { Link } from 'react-router-dom';\n\nfunction Landing() {\n  return (\n    <div>\n      <header className=\"header\">\n        <nav className=\"navbar\">\n          <div className=\"logo\">\n            <i className=\"fas fa-baby\"></i>\n            <span className=\"logo-text\">IncuNeo-I</span>\n          </div>\n          <ul className=\"nav-links\">\n            <li><a href=\"#features\">Fonctionnalités</a></li>\n            <li><a href=\"#about\">À propos</a></li>\n            <li><a href=\"#contact\">Contact</a></li>\n            <li><Link to=\"/login\" className=\"btn-login\">Connexion</Link></li>\n          </ul>\n        </nav>\n      </header>\n\n      <main>\n        <section className=\"hero\">\n          <div className=\"hero-content\">\n            <h1>Surveillance Médicale Intelligente</h1>\n            <p>Un système avancé de monitoring pour incubateurs néonatals, conçu pour protéger et suivre les nouveau-nés.</p>\n            <div className=\"hero-cta\">\n              <a href=\"#features\" className=\"btn btn-primary\">Découvrir</a>\n              <Link to=\"/login\" className=\"btn btn-secondary\">Se Connecter</Link>\n            </div>\n          </div>\n          <div className=\"hero-image\">\n            <img src={imgIncu} alt=\"Incubateur néonatal\" />\n          </div>\n        </section>\n\n        <section id=\"features\" className=\"features\">\n          <h2>Nos Fonctionnalités Principales</h2>\n          <div className=\"feature-grid\">\n            <div className=\"feature-card\">\n              <i className=\"fas fa-heartbeat\"></i>\n              <h3>Monitoring en Temps Réel</h3>\n              <p>Suivi constant des paramètres vitaux des nouveau-nés.</p>\n            </div>\n            <div className=\"feature-card\">\n              <i className=\"fas fa-temperature-low\"></i>\n              <h3>Contrôle Thermique</h3>\n              <p>Maintien précis de la température optimale.</p>\n            </div>\n            <div className=\"feature-card\">\n              <i className=\"fas fa-mobile-alt\"></i>\n              <h3>Accès Mobile</h3>\n              <p>Surveillance à distance via notre application.</p>\n            </div>\n          </div>\n        </section>\n\n        <section id=\"about\" className=\"about\">\n          <div className=\"about-content\">\n            <h2>Notre Mission</h2>\n            <p>IncuNeo-I est né de la volonté de fournir aux familles et aux professionnels de santé un outil technologique de pointe pour accompagner les nouveau-nés les plus fragiles.</p>\n            <p>Notre système combine précision médicale et compassion technologique.</p>\n          </div>\n        </section>\n\n        <section id=\"contact\" className=\"contact\">\n          <h2>Contactez-nous</h2>\n          <form className=\"contact-form\">\n            <input type=\"text\" placeholder=\"Votre nom\" required />\n            <input type=\"email\" placeholder=\"Votre email\" required />\n            <textarea placeholder=\"Votre message\" required></textarea>\n            <button type=\"submit\" className=\"btn btn-primary\">Envoyer</button>\n          </form>\n        </section>\n      </main>\n\n      <footer>\n        <div className=\"footer-content\">\n          <p>&copy; 2024 IncuNeo-I. Tous droits réservés.</p>\n          <div className=\"footer-links\">\n            <a href=\"#\">Mentions légales</a>\n            <a href=\"#\">Politique de confidentialité</a>\n          </div>\n        </div>\n      </footer>\n    </div>\n  );\n}\nexport default Landing\n","C:\\Users\\Merveille ALLADATIN\\Desktop\\Stage et Memoire\\IncuNeoReact\\incu-neo-i\\src\\components\\LoginPage.js",[],"C:\\Users\\Merveille ALLADATIN\\Desktop\\Stage et Memoire\\IncuNeoReact\\INCU-NEO-I\\src\\index.js",[],["142","143"],"C:\\Users\\Merveille ALLADATIN\\Desktop\\Stage et Memoire\\IncuNeoReact\\INCU-NEO-I\\src\\App.js",[],"C:\\Users\\Merveille ALLADATIN\\Desktop\\Stage et Memoire\\IncuNeoReact\\INCU-NEO-I\\src\\components\\LoginPage.js",["144","145","146"],"// src/pages/LoginPage.js\nimport { useState } from 'react';\nimport { useNavigate } from 'react-router-dom'; // Import de useNavigate\nimport \"../styles/Login.css\";\n\nfunction LoginPage() {\n  const [userType, setUserType] = useState('parent');\n  const [username, setUsername] = useState('');\n  const [password, setPassword] = useState('');\n  const [twoFactorCode, setTwoFactorCode] = useState('');\n  const [error, setError] = useState(false);\n  const navigate = useNavigate(); // Hook pour naviguer entre les pages\n\n  const handleUserTypeChange = (type) => {\n    setUserType(type);\n    setError(false); // reset erreur à chaque changement\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n\n    if (!username || !password) {\n      setError(true);\n      return;\n    }\n\n    // Simuler une API login\n    setTimeout(() => {\n      setError(false);\n      switch (userType) {\n        case 'medical':\n          // Rediriger vers le tableau de bord médical\n          navigate('/dashboard');\n          break;\n        case 'parent':\n          // Rediriger vers le tableau de bord parent\n          navigate('/dashboard-parent');\n          break;\n        case 'admin':\n          // Rediriger vers le tableau de bord admin\n          navigate('/dashboard-admin');\n          break;\n        default:\n          setError(true);\n      }\n    }, 1000);\n  };\n\n  return (\n    <div className=\"container\">\n      <div className=\"landing-image\">\n        <div className=\"image-overlay\"></div>\n        <div className=\"brand-container\">\n          <div className=\"brand-logo\">\n            <i className=\"fas fa-baby\"></i>\n          </div>\n          <h1 className=\"brand-name\">IncuNeo-I</h1>\n          <p className=\"brand-tagline\">Système de surveillance avancé pour incubateurs néonatals</p>\n        </div>\n      </div>\n\n      <div className=\"login-container\">\n        <div className=\"login-box\">\n          <div className=\"login-header\">\n            <h2 className=\"login-title\">Connexion</h2>\n            <p className=\"login-subtitle\">Accédez à votre espace sécurisé</p>\n          </div>\n\n          {error && (\n            <div className=\"alert\">\n              Les identifiants saisis sont incorrects ou incomplets. Veuillez réessayer.\n            </div>\n          )}\n\n          <div className=\"user-type-selector\">\n            {['medical', 'parent', 'admin'].map((type) => (\n              <button\n                key={type}\n                className={`user-type-btn ${userType === type ? 'active' : ''}`}\n                onClick={() => handleUserTypeChange(type)}\n              >\n                <i className={`fas ${\n                  type === 'medical' ? 'fa-user-md' :\n                  type === 'parent' ? 'fa-users' : 'fa-cog'\n                }`}></i>\n                {type === 'medical' && ' Personnel médical'}\n                {type === 'parent' && ' Parents'}\n                {type === 'admin' && ' Admin'}\n              </button>\n            ))}\n          </div>\n\n          <form onSubmit={handleSubmit}>\n            <div className=\"form-group\">\n              <label htmlFor=\"username\" className=\"form-label\">Identifiant</label>\n              <input\n                type=\"text\"\n                id=\"username\"\n                className=\"form-input\"\n                placeholder=\"Entrez votre identifiant\"\n                value={username}\n                onChange={(e) => setUsername(e.target.value)}\n                required\n              />\n            </div>\n\n            <div className=\"form-group\">\n              <label htmlFor=\"password\" className=\"form-label\">Mot de passe</label>\n              <input\n                type=\"password\"\n                id=\"password\"\n                className=\"form-input\"\n                placeholder=\"Entrez votre mot de passe\"\n                value={password}\n                onChange={(e) => setPassword(e.target.value)}\n                required\n              />\n              <a href=\"#\" className=\"forgot-password\">Mot de passe oublié ?</a>\n            </div>\n\n            {userType === 'medical' && (\n              <div className=\"form-group\">\n                <label htmlFor=\"two-factor\" className=\"form-label\">Code d'authentification</label>\n                <input\n                  type=\"text\"\n                  id=\"two-factor\"\n                  className=\"form-input\"\n                  placeholder=\"Entrez le code reçu\"\n                  value={twoFactorCode}\n                  onChange={(e) => setTwoFactorCode(e.target.value)}\n                />\n              </div>\n            )}\n\n            <button type=\"submit\" className=\"btn\">Se connecter</button>\n          </form>\n\n          <div className=\"login-footer\">\n            <p>\n              En vous connectant, vous acceptez nos{' '}\n              <a href=\"#\">conditions d'utilisation</a> et notre{' '}\n              <a href=\"#\">politique de confidentialité</a>.\n            </p>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default LoginPage;\n","C:\\Users\\Merveille ALLADATIN\\Desktop\\Stage et Memoire\\IncuNeoReact\\INCU-NEO-I\\src\\components\\Landing.js",["147","148"],"C:\\Users\\Merveille ALLADATIN\\Desktop\\Stage et Memoire\\IncuNeoReact\\incu-neo-i\\src\\components\\dashboard\\medical\\MedicalDashboard.js",[],"C:\\Users\\Merveille ALLADATIN\\Desktop\\Stage et Memoire\\IncuNeoReact\\incu-neo-i\\src\\components\\dashboard\\medical\\Sidebar.js",["149"],"// src/components/dashboard/medical/Sidebar.js\nimport React, { useState, useEffect } from 'react';\nimport { Link, useNavigate } from 'react-router-dom';\nimport '../../../styles/dashboard/Components.css';\n\